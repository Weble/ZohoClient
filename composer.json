{
  "name": "weble/zohoclient",
  "description": "Zoho API Client for oAuth auth and common tasks - PHP SDK",
  "license": "MIT",
  "keywords": [
    "zoho",
    "api",
    "oauth2"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Daniele Rosario",
      "email": "daniele@weble.it"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3",
    "tightenco/collect": "^6.0",
    "psr/cache": "^1.0",
    "bentools/guzzle-throttle-middleware": "^0.4.0",
    "spatie/enum": "^2.3",
    "cache/filesystem-adapter": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "7.*",
    "vlucas/phpdotenv": "^4.1",
    "cache/array-adapter": "^1.0",
    "phpstan/phpstan": "^0.12.3"
  },
  "autoload": {
    "psr-4": {
      "Weble\\ZohoClient\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
